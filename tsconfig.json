{
  "compilerOptions": {
    "noEmitOnError": false,
    "skipLibCheck": true, 
    "strict": false,  
    "forceConsistentCasingInFileNames": false,
    "jsx": "react-jsx",
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "noEmit": true 
  },
  "include": ["src"]
}
